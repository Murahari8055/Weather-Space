{
  "version": 2,
  "builds": [
    {
      "src": "angular.json",
      "use": "@vercel/cli"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "http://13.62.100.171:8080/api/:path*"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
